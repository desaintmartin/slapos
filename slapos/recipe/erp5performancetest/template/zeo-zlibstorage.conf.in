# ZEO configuration file generated by SlapOS
%import zc.zlibstorage

<zeo>
  address %(zeo_ip)s:%(zeo_port)s
  pid-filename %(zeo_pid)s
</zeo>

<serverzlibstorage>
  %(zeo_filestorage_snippet)s
</serverzlibstorage>

<eventlog>
  <logfile>
    dateformat
    path %(zeo_event_log)s
  </logfile>
</eventlog>
